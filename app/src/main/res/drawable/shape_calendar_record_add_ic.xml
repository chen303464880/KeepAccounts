<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape
            android:innerRadius="24dp"
            android:shape="ring"
            android:thickness="1dp"
            android:useLevel="false">
            <solid android:color="@color/text_color_655f5f" />
            <size
                android:width="50dp"
                android:height="50dp" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <solid android:color="@color/text_color_655f5f" />
            <size
                android:width="1dp"
                android:height="20dp" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <solid android:color="@color/text_color_655f5f" />
            <size
                android:width="20dp"
                android:height="1dp" />
        </shape>
    </item>
</layer-list>